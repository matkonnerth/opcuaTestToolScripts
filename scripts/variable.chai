def intent(x)
{
  for(auto i=0; i < x; ++i)
  {
    puts(" ");
  }
}

def browseRecursive(id, intention) 
{
  auto results = browse(id);
  intent(intention)
   for(r : results)
   {
      print(r.Name().Name())
      auto nextIntent = intention+1
      browseRecursive(r.Id(), nextIntent)
   }
}

browseRecursive(NodeId(0,85), 0)

